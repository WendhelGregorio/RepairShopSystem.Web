@model ProjectBSIS401.WEB.Infrastructures.Domain.Models.Pricing
@{
    Layout = "~/areas/manage/views/shared/_forms.cshtml";
}
<div class="section-title">
    <h2>Details</h2>
    <p></p>
</div>

<div>
    <h4>Pricing details</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Title
        </dt>
        <dd class="col-sm-10">
            @Model.Title
        </dd>
        <dt class="col-sm-2">
            Price
        </dt>
        <dd class="col-sm-10">
            @Model.Price
        </dd>
        <dt class="col-sm-2">
            Month
        </dt>
        <dd class="col-sm-10">
            @Model.Month
        </dd>
        <dt class="col-sm-2">
            Content List
        </dt>
        @foreach (var content in Model.PricingContents)
        {
            <dd>
                <ol class="list-group list-group-numbered">
                    <li class="list-group-item">@content.Content</li>
                 
                </ol>
            </dd>
        }

    </dl>
</div>
<div>
    <a href="~/manage/pricings/edit/@Model.Id" class="btn btn-primary" asp-route-id="@Model.Id">Edit</a>
    <a href="~/manage/pricings/index" class="btn btn-danger">Back to List</a>
</div>